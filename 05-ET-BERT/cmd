                                        --dev_path datasets/vpn_own/XY_ETBERT_encapsulation_test.tsv \
                                        --test_path datasets/vpn_own/XY_ETBERT_encapsulation_test.tsv \
                                        --epochs_num 100 \
                                        --batch_size 32 \
                                        --embedding word_pos_seg \
                                        --encoder transformer \
                                        --mask fully_visible \
                                        --seq_length 128 \
                                        --learning_rate 2e-5 \
                                        --gpu 0 \
                                        --output_model_path models/vpn_own/encapsulation.bin 

