python3 fine-tuning/run_classifier.py   --pretrained_model_path models/pretrained_model_cst.bin \
                                        --vocab_path models/encryptd_vocab.txt \
                                        --train_path datasets/vpn_use_tcp_8/XY_ETBERT_encapsulation_train.tsv \
                                        --dev_path datasets/vpn_use_tcp_8/XY_ETBERT_encapsulation_test.tsv \
                                        --test_path datasets/vpn_use_tcp_8/XY_ETBERT_encapsulation_test.tsv \
                                        --epochs_num 20 \
                                        --batch_size 90 \
                                        --embedding word_pos_seg \
                                        --encoder transformer \
                                        --mask fully_visible \
                                        --seq_length 128 \
                                        --learning_rate 2e-5 \
                                        --gpu 0 \
                                        --output_model_path models/ETBERT_vpn_encapsulation.bin 

